<% if params[:tag] == "Kelany" %>
  <% if params[:rotate] == "neg" %>
    <section class="wow fadeIn parallax" data-stellar-background-ratio="0.1" style="background-image:url('<%= image_path('pofo/world-kelany-neg.png') %>');">
  <% else %>
    <section class="wow fadeIn parallax" data-stellar-background-ratio="0.1" style="background-image:url('<%= image_path('pofo/world-kelany-pos.png') %>');">
  <% end %>
<% elsif params[:tag] == "Bahonu" %>
